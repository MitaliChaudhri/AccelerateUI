<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    	xmlns:l="sap.ui.layout"
>
    <Dialog showHeader="true"  contentWidth="60%" contentHeight="30%">
    		
           	<VBox class="sapUiSmallMargin">
		<f:Form 
			editable="true">
			<f:title>
				<core:Title text="Add Business Sub Process" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="4"
					labelSpanL="4"
					labelSpanM="12"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="0"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="2"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer title="">
					<f:formElements>
						<f:FormElement label="Business Process">
							<f:fields>
								<Input value="{SupplierName}"/>
							</f:fields>
						</f:FormElement>
                        <f:FormElement label="">
						</f:FormElement>
							<f:FormElement label="Business Sub Process Name">
							<f:fields>
								<Select >
									<items>
										
									</items>
								</Select>
							</f:fields>
						</f:FormElement>
						
					
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer title="">
					<f:formElements>
							<f:FormElement label="Business Sub Process Description">
							<f:fields>
								<Input value=""></Input>
							</f:fields>
						</f:FormElement>
 <f:FormElement label="">
						</f:FormElement>

                  
				
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
  <footer>
            <OverflowToolbar>
                <ToolbarSpacer />

                <Button
                  text="Save"
                    type="Accept"
                    press="OnSaveBusinessSubProcess"
                />
                   <Button
                  text="Close"
                    type="Reject"
                    press="OnCloseBusinessSubProcess"
                />
               
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>